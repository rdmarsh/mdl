- content_for(:edit_button_path, edit_device_path(@device))

= render 'head'

= show_for @device do |f|
  = f.attribute :name
  = f.attribute :name, :in => :service_level
  = f.attribute :name, :in => :support_level
  = f.attribute :name, :in => :device_model
  = f.attribute :name, :in => :environment
  = f.attribute :name, :in => :organisation
  = f.attribute :description
  = f.attribute :created_at, :format => :iso8601
  = f.attribute :updated_at, :format => :iso8601
